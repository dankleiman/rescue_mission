<!DOCTYPE html>
<html>
<head>
  <title>Rescue Mission</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <div id="user_nav">
    <%= link_to "Sign in with Facebook", "/auth/facebook"%>
  </div>
  <% if flash.any? %>
  <div>
    <%= flash[:notice] %>
  </div>
<% end %>

<%= yield %>

</body>
</html>
